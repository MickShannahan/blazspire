{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "app-build",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"-c",
				"Debug",
			],
			"isBackground": false,
			"problemMatcher": "$msCompile",
		},
		{
			"label": "package",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"${workspaceFolder}",
				"-c",
				"Debug",
				"-f",
				"net9.0-windows10.0.19041.0",
				"-p:WindowsPackageType=Msix"
			]
		}
	]
}